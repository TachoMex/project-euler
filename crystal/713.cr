def t(n, m)
  q = n // (m - 1)
  r = n % (m - 1)
  (m - 1) * q * (q - 1) // 2 + r * q
  # k = r - 1
  # puts "#{n} #{r} #{k}"
  # n * (n - 1) // 2 - (k - 1) * n * n // (2 * k)
end

